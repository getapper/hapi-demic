{
  "api": {
    "routes": {
      "$id": {
        "methods": {
          "GET": "api/$id/GET"
        }
      },
      "errors": {
        "methods": {
          "POST": "api/errors/POST"
        }
      },
      "project": {
        "methods": {
          "GET": "api/project/GET",
          "POST": "api/project/POST"
        }
      },
      "routing": {
        "methods": {
          "GET": "api/routing/GET",
          "POST": "api/routing/POST"
        },
        "routes": {
          "endpoints": {
            "methods": {
              "POST": "api/routing/endpoints/POST"
            }
          }
        }
      }
    }
  }
}